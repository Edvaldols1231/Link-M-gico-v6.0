{
  "name": "linkmagico-v6-plus-v7-integrated",
  "version": "6.7.1",
  "description": "Link Mágico v6.0 + v7.0 Integrado - IA Conversacional com Demos Públicos, Dashboard Analytics e Widget Embedável",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --test",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "status": "curl -s http://localhost:3000/status | jq .",
    "logs": "tail -f app.log",
    "deploy-check": "node scripts/deploy-check.js",
    "lint": "eslint .",
    "render-build": "npm install && npm run build || true"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "body-parser": "^1.20.2",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0"
  },
  "optionalDependencies": {
    "puppeteer": "^21.0.0",
    "tesseract.js": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "linkmagico",
    "chatbot",
    "ai-assistant",
    "groq-api",
    "openai",
    "openrouter",
    "widget-embedable",
    "dashboard-analytics",
    "demos-publicos",
    "rate-limiting",
    "fallback-system",
    "conversational-ai",
    "nodejs-express",
    "render-deployment"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edvaldols1231/Link-M-gico-v6.0.git"
  },
  "author": {
    "name": "Edvaldo",
    "email": "contato@linkmagico.com",
    "url": "https://link-m-gico-v6-0-dx0q.onrender.com"
  },
  "license": "MIT",
  "homepage": "https://link-m-gico-v6-0-dx0q.onrender.com",
  "bugs": {
    "url": "https://github.com/Edvaldols1231/Link-M-gico-v6.0/issues",
    "email": "suporte@linkmagico.com"
  },
  "funding": {
    "type": "individual",
    "url": "https://link-m-gico-v6-0-dx0q.onrender.com/docs"
  },
  "config": {
    "port": 3000,
    "logLevel": "info",
    "rateLimit": {
      "dashboard": 50,
      "widget": 200,
      "api": 100
    }
  },
  "api_providers": {
    "primary": "GROQ",
    "fallback_order": [
      "OpenAI",
      "OpenRouter",
      "Native"
    ],
    "endpoints": {
      "chat": "/chat-universal",
      "extract": "/extract",
      "status": "/status"
    }
  }
}
