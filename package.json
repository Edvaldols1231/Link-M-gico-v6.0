{
  "name": "linkmagico-v6-plus-v7-integrated",
  "version": "6.7.0",
  "description": "Link Mágico v6.0 + v7.0 Integrado - IA Conversacional com Demos Públicos, Dashboard Analytics e Widget Embedável",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --test",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "status": "curl -s http://localhost:3000/status | jq .",
    "logs": "tail -f app.log",
    "deploy-check": "node scripts/deploy-check.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "body-parser": "^1.20.2",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "linkmagico",
    "chatbot",
    "ai-assistant",
    "groq-api",
    "openai",
    "openrouter",
    "widget-embedable",
    "dashboard-analytics",
    "demos-publicos",
    "rate-limiting",
    "fallback-system",
    "conversational-ai",
    "nodejs-express",
    "render-deployment"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edvaldols1231/Link-M-gico-v6.0.git"
  },
  "author": {
    "name": "Edvaldo",
    "email": "contato@linkmagico.com",
    "url": "https://link-m-gico-v6-0-dx0q.onrender.com"
  },
  "license": "MIT",
  "homepage": "https://link-m-gico-v6-0-dx0q.onrender.com",
  "bugs": {
    "url": "https://github.com/Edvaldols1231/Link-M-gico-v6.0/issues",
    "email": "suporte@linkmagico.com"
  },
  "funding": {
    "type": "individual",
    "url": "https://link-m-gico-v6-0-dx0q.onrender.com/docs"
  },
  "config": {
    "port": 3000,
    "logLevel": "info",
    "rateLimit": {
      "dashboard": 50,
      "widget": 200,
      "api": 100
    }
  },
  "demo_urls": {
    "production": "https://link-m-gico-v6-0-dx0q.onrender.com",
    "homepage": "/",
    "dashboard_demo": "/dashboard/demo",
    "widget_demo": "/widget/demo",
    "widget_embed": "/widget.js",
    "api_status": "/api/public/status",
    "docs": "/docs",
    "health": "/health",
    "analytics": "/analytics"
  },
  "api_providers": {
    "primary": "GROQ",
    "fallback_order": [
      "OpenAI",
      "OpenRouter", 
      "Native"
    ],
    "endpoints": {
      "chat": "/chat-universal",
      "extract": "/extract",
      "status": "/status"
    }
  },
  "features": {
    "v6_original": {
      "chat_universal": true,
      "data_extraction": true,
      "analytics_dashboard": true,
      "multi_ai_support": true,
      "fallback_system": true
    },
    "v7_integrated": {
      "public_demos": true,
      "dashboard_demo": true,
      "widget_embedable": true,
      "rate_limiting": true,
      "structured_logging": true,
      "api_documentation": true,
      "health_monitoring": true
    }
  },
  "rate_limits": {
    "dashboard_demo": {
      "window": "10min",
      "max": 50,
      "description": "Dashboard demo público"
    },
    "widget_embed": {
      "window": "5min", 
      "max": 200,
      "description": "Widget embedável"
    },
    "api_public": {
      "window": "15min",
      "max": 100,
      "description": "API pública para testes"
    },
    "chat_universal": {
      "window": "15min",
      "max": 100,
      "description": "Chat com IA"
    },
    "general": {
      "window": "1min",
      "max": 1000,
      "description": "Proteção DDoS"
    }
  },
  "environment_variables": {
    "required": [
      "PORT",
      "NODE_ENV"
    ],
    "api_keys": [
      "GROQ_API_KEY",
      "OPENAI_API_KEY", 
      "OPENROUTER_API_KEY"
    ],
    "optional": [
      "RENDER_EXTERNAL_URL",
      "ALLOWED_EMBED_DOMAINS"
    ]
  },
  "deployment": {
    "platform": "render",
    "build_command": "npm install",
    "start_command": "npm start",
    "health_check": "/health",
    "auto_deploy": true
  },
  "monitoring": {
    "logs": {
      "file": "app.log",
      "level": "info",
      "format": "json"
    },
    "metrics": {
      "uptime": "/status",
      "health": "/health",
      "analytics": "/api/public/status"
    }
  },
  "security": {
    "helmet": true,
    "cors": true,
    "rate_limiting": true,
    "input_validation": true,
    "api_fallbacks": 3
  },
  "performance": {
    "compression": true,
    "static_cache": true,
    "request_timeout": "15s",
    "response_cache": "30min"
  },
  "widget_integration": {
    "file": "/widget.js",
    "size": "~5KB",
    "load_time": "<2s",
    "mobile_responsive": true,
    "customizable": true,
    "embed_code": "Disponível em /widget/demo"
  },
  "api_compatibility": {
    "groq": {
      "model": "mixtral-8x7b-32768",
      "timeout": "10s",
      "priority": 1
    },
    "openai": {
      "model": "gpt-3.5-turbo",
      "timeout": "10s", 
      "priority": 2
    },
    "openrouter": {
      "model": "microsoft/wizardlm-2-8x22b",
      "timeout": "10s",
      "priority": 3
    },
    "native": {
      "fallback": true,
      "priority": 4
    }
  },
  "browser_support": {
    "chrome": ">=60",
    "firefox": ">=55", 
    "safari": ">=12",
    "edge": ">=79",
    "mobile": "iOS 12+, Android 7+"
  },
  "integration_examples": {
    "widget_basic": "Código em /widget/demo",
    "api_curl": "Exemplos em /docs",
    "dashboard": "Demo em /dashboard/demo"
  },
  "changelog": {
    "6.7.0": "Integração completa v6.0 + v7.0",
    "6.0.0": "Versão original estável",
    "7.0.0": "Demos públicos e widget"
  },
  "roadmap": {
    "próximas_features": [
      "Dashboard customizável",
      "Múltiplos idiomas", 
      "Analytics avançadas",
      "Integrações webhook"
    ]
  }
}